{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="{{ page_type }}">
	<div class="left">
	
		<div class="logo">
			<img src="/wp-content/themes/dikeou-collection/static/images/logo.svg" />
		</div>
		<nav>
			<div class="nav-inner">
			<ul class="nav-links"">
				<a href="/artists" class="{% if page_type == 'artists' %}active{% endif %}"><li>Artists</li></a>
				<a href="/events"><li>Events</li></a>
				<a href="/contatc"><li>Contact</li></a>
				<a href="/about"><li>About</li></a>
